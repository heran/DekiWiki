      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="SgmlReaderDll" publicKeyToken="46b2db9ca481831b" culture="neutral" />
        <bindingRedirect oldVersion="1.8.0.0-1.8.8.42163" newVersion="1.8.8.42163" />
